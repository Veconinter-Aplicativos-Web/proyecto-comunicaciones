(function ($){
    $("#send").click(function(){
        //alert ('her comes');
        $("#divError").html('<img src="/img/ajax-loader.gif" />');
        //$("#divError").html('');
        var boton='<button id="send" name="send" class="send_button"><i class="ico-check3"></i><span>Send Your Message</span></button>';
        $.post("/public/contact/ajaxGuardar",
            {
                name: $("#name").val(),
                email: $("#email").val(),
                subject: $("#subject").val(),
                comment: $("#comment").val(),
                topic: $("#topic").val()

            },
            function(data,status){
                if (status=='success'){
                    if (data==1){
                        $("#divButton").html('');
                        $("#divError").html('<div style="font-weight: bold; color: #00CA33">Message sent successfully!</div>');
                    }else if(data==3){
                        $("#divError").html('<div style="font-weight: bold; color: red">Message not sent:</div><div style="color: red">You must complete all the fields</div>');
                    }else if(data==2){
                        $("#divError").html('<div style="font-weight: bold; color: red">Message not sent:</div><div style="color: red">Error saving</div>');
                    }else{
                        $("#divError").html('<div style="font-weight: bold; color: red">Message not sent:</div><div style="color: red">Error unknown</div>');
                    }
                }else{
                    $("#divError").html('<div style="font-weight: bold; color: red">Message not sent:</div><div style="color: red">Error unknown return</div>');
                }

            });
    });
